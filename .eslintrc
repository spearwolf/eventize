{
  "plugins": ["jest"],
  "extends": ["airbnb-base"],
  "rules": {
    "max-len": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-continue": 0,
    "default-case": 0,
    "consistent-return": 0,
    "no-multi-spaces": ["error", {
      "exceptions": {
        "VariableDeclarator": true
      }
    }]
  }
}
